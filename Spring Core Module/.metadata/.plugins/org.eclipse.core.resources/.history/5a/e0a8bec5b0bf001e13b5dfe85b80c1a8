log4j.rootLogger = INFO, CA

log4j.appender.CA = org.apache.log4j.ConsoleAppender
log4j.appender.CA.layout = org.apache.log4j.HTMLLayout
log4j.appender.CA.layout.ConversionPattern = [%p] %d %m%n


# log4j.appender.FA.File -> Specifies where we want to create log file
# If we do not provide ConversionPattern then default one (i.e. %m) is used