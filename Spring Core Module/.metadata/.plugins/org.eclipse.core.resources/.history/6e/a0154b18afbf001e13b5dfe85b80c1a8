log4j.rootLogger = INFO, FA

log4j.appender.FA = org.apache.log4j.FileAppender
log4j.appender.FA.File = D://myLog4j.log
log4j.appender.CA.layout = org.apache.log4j.PatternLayout
log4j.appender.CA.layout.ConversionPattern = [%p] %d %m%n


# log4j.appender.FA.File -> Specifies where we want to create log file